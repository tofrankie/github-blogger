{
  "name": "github-blogger",
  "displayName": "GitHub Blogger",
  "description": "Writing and managing blog posts via GitHub issue",
  "version": "2.4.0",
  "packageManager": "pnpm@10.17.0",
  "publisher": "Frankie",
  "author": {
    "name": "Frankie",
    "email": "1426203851@qq.com"
  },
  "type": "module",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Notebooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tofrankie/github-blogger.git"
  },
  "activationEvents": [],
  "main": "./dist/extension/index.js",
  "icon": "./images/icon.png",
  "contributes": {
    "configuration": {
      "title": "GitHub Blogger",
      "properties": {
        "github-blogger.token": {
          "type": "string",
          "description": "Your GitHub Personal Access Token (classic)",
          "default": ""
        },
        "github-blogger.user": {
          "type": "string",
          "description": "Your GitHub Username",
          "default": ""
        },
        "github-blogger.repo": {
          "type": "string",
          "description": "Your Blog Repository Name",
          "default": ""
        },
        "github-blogger.branch": {
          "type": "string",
          "description": "Your Blog Repository Branch Name",
          "default": "main"
        }
      }
    },
    "commands": [
      {
        "command": "github-blogger.open",
        "title": "Open GitHub Blogger"
      },
      {
        "command": "github-blogger.configure",
        "title": "Configure GitHub Blogger"
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "package": "vsce package --no-dependencies",
    "package:pre": "vsce package --pre-release --no-dependencies",
    "prepackage": "pnpm build",
    "prepackage:pre": "pnpm build",
    "ext:publish": "vsxpub --no-dependencies --exclude git",
    "lint": "run-s lint:eslint lint:prettier lint:stylelint tsc",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --cache --fix",
    "lint:prettier": "prettier . '!**/pnpm-lock.yaml' --cache --write",
    "lint:stylelint": "stylelint 'src/**/*.css' --cache --fix",
    "tsc": "tsc --noEmit",
    "check": "run-s tsc lint",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@octokit/request-error": "7.0.0",
    "@octokit/types": "15.0.0",
    "@tanstack/eslint-plugin-query": "5.89.0",
    "@tomjs/commitlint": "4.0.0",
    "@tomjs/eslint": "5.2.0",
    "@tomjs/prettier": "2.0.0",
    "@tomjs/stylelint": "6.0.0",
    "@tomjs/tsconfig": "2.0.0",
    "@tomjs/vite-plugin-vscode": "5.0.0",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/styled-components": "5.1.34",
    "@types/vscode": "1.88.0",
    "@types/vscode-webview": "1.57.5",
    "@vitejs/plugin-react": "5.0.3",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vscode/vsce": "3.6.0",
    "eslint": "9.36.0",
    "lint-staged": "16.1.6",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "simple-git-hooks": "2.13.1",
    "stylelint": "16.24.0",
    "typescript": "5.9.2",
    "vite": "6.3.5",
    "vsxpub": "0.1.1"
  },
  "dependencies": {
    "@bytemd/plugin-breaks": "1.22.0",
    "@bytemd/plugin-frontmatter": "1.22.0",
    "@bytemd/plugin-gemoji": "1.22.0",
    "@bytemd/plugin-gfm": "1.22.0",
    "@bytemd/plugin-highlight": "1.22.0",
    "@bytemd/plugin-math": "1.22.0",
    "@bytemd/plugin-medium-zoom": "1.22.0",
    "@bytemd/plugin-mermaid": "1.22.0",
    "@bytemd/react": "1.22.0",
    "@octokit/core": "7.0.4",
    "@primer/css": "22.0.2",
    "@primer/octicons-react": "19.18.0",
    "@primer/primitives": "11.1.0",
    "@primer/react": "37.31.0",
    "@tanstack/react-query": "5.89.0",
    "@tanstack/react-query-devtools": "5.89.0",
    "@vscode/webview-ui-toolkit": "1.4.0",
    "buffer": "6.0.3",
    "bytemd": "1.22.0",
    "core-js": "3.45.1",
    "dayjs": "1.11.18",
    "github-markdown-css": "5.8.1",
    "gray-matter": "4.0.3",
    "highlight.js": "11.11.1",
    "js-base64": "3.7.8",
    "licia": "1.48.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "styled-components": "5.3.11",
    "vscode-webview-rpc": "0.0.4",
    "zustand": "5.0.8"
  }
}
